apiVersion: autoscaling/v1
kind: HorizontalPodAutoscaler
metadata:
  name: '{SITENAME}-{BRANCH}-{ENVIRONMENT}'
spec:
  maxReplicas: 6
  minReplicas: 2
  scaleTargetRef:
    apiVersion: extensions/v1beta1
    kind: Deployment
    name: '{SITENAME}-{BRANCH}-{ENVIRONMENT}'
  targetCPUUtilizationPercentage: 60